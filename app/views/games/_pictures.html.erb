<div class="main-container bg-image", style="background-image: url(<%= image_path 'gradient-ce.png' %>);">
  <p data-action="click->map#hide" class="picture-btn"><i class="fa-regular fa-map" style="color: #fffff;"></i></p>

    <div data-controller="swiper" class="swiper mySwiper">

      <div data-swiper-target="card" class="swiper-wrapper">
        <% @places.each do |place|%>
          <div class="swiper-slide">
            <img src=<%= place.url_image %> data-action="click->map#select" data-map-place-id-param="<%= place.id %>">
          </div>
        <% end %>
        <div class="swiper-slide opacity-0"></div>
        </div>
      </div>
    </div>
</div>
